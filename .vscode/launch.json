    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "env": {
                "CONFIG_FILE": "${fileWorkspaceFolder}/src/shift_left/tests/config.yaml",
                "PYTHONPATH": "${fileWorkspaceFolder}/src/shift_left/src:${fileWorkspaceFolder}/src/shift_left/tests"
              },
               "python": "${workspaceFolder}/src/shift_left/.venv/bin/python"
        },
        {
            "name": "Debugger: mc-project",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "CONFIG_FILE": "/Users/jerome/.shift_left/config-stage-2b-flink.yaml",
                "PIPELINES": "/Users/jerome/Code/customers/mc/data-platform-flink/pipelines",
                "PYTHONPATH": "${workspaceFolder}/src/shift_left/src:${workspaceFolder}/src/shift_left/tests"
            },
             "python": "${workspaceFolder}/src/shift_left/.venv/bin/python"
        },
        {
            "name": "Python IT Debugger: current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "env": {
                "CONFIG_FILE": "${fileWorkspaceFolder}/src/shift_left/tests/config-ccloud.yaml",
                "PYTHONPATH": "${fileWorkspaceFolder}/src/shift_left/src:${fileWorkspaceFolder}/src/shift_left/tests"
              },
               "python": "${workspaceFolder}/src/shift_left/.venv/bin/python"
        },
    ]
}